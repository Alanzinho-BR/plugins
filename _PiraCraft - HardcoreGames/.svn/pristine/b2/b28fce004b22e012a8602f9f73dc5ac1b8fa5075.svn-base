	@EventHandler
	public void Kits(PlayerInteractEvent e) {
		Player p = e.getPlayer();
		if (p.getItemInHand().getType() == Material.CHEST) {
			if (Main.PreGame) {
				new Inventario(p).openInv(1);
			}
		}
	}

